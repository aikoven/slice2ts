{
  "name": "slice2ts",
  "version": "0.2.0",
  "bin": {
    "slice2ts": "lib/cli.js"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "test": "jest",
    "build": "tsc -P tsconfig.build.json",
    "prepublishOnly": "npm test && npm run clean && npm run build"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": "aikoven/slice2ts",
  "author": "Daniel Lytkin <dan.lytkin@gmail.com>",
  "license": "GPL-2.0",
  "dependencies": {
    "assert-never": "^1.2.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "commander": "^9.3.0",
    "commondir": "^1.0.1",
    "escape-string-regexp": "^2.0.0",
    "glob": "^7.1.4",
    "mkdirp": "^0.5.1",
    "prettier": "^1.18.2",
    "slice2js": "~3.7.2",
    "slice2json": "^0.3.1",
    "unixify": "^1.0.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/babel-core": "^6.25.6",
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.15",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^12.6.8",
    "@types/prettier": "^1.16.4",
    "@types/uuid": "^3.4.5",
    "jest": "^24.8.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
